<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goooglemapss">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Rent_Property"
            android:label="@string/title_activity_rent__property"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Sell_Property"
            android:label="@string/title_activity_sell__property"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".buy_property"
            android:label="@string/title_activity_buy_property"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Welcome_Page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>